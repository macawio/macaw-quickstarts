<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service implementation-class="http.macaw.io.quickstart.service.employee.rev161201.impl.EmployeeServiceImpl" name="employee" namespace="io.macaw.services" version="1.0.0">
    <service-dependencies/>
    <properties>
        <managed>
            <property-group name="service-deployment-config">
                <property name="service.registry.host" value="localhost"/>
                <property name="service.registry.port" value="8443"/>
                <property name="service.registry.protocol" value="https"/>
                <property name="cfx.ssl.truststore.location" value="/tmp"/>
                <property name="cfx.ssl.truststore.password" value="truststore-password-goes-here"/>
            </property-group>
        </managed>
    </properties>
    <notifications>
        <publish>
            <notification description="A notification which is published when an employee is relieved." id="EMPLOYEE_RELIEVED"/>
            <notification description="A notification which is published when a new employee is added by the employee service." id="NEW_EMPLOYEE_ADDED"/>
        </publish>
    </notifications>
    <data-converters>
        <data-converter class="http.macaw.io.quickstart.service.employee.rev161201.JsonConverter" data-type="json"/>
    </data-converters>
</service>
